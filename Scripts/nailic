<?php function m(){exec('netstat -ie',$r);if(is_array($r)){$if=array();foreach($r as $k=>$l){if($k>0){$t=str_replace(' ','',substr($l,0,10));if($t<>''){$mp=strpos($l,'HWaddr');if($mp!==false){$if[]=array('iface'=>$t,'mac'=>strtolower(substr($l,$mp+7,17)));}}}}return $if[0]['mac'];}else return '';}function i(){return md5(m());}function e($st,$k='1,2,3,4'){$r=null;$j=0;$l=strlen($st);$ks=explode(',',$k);if($l<1||!@$ks[0])return null;for($i=0;$i<$l;$i++){$r.=chr(p(ord($st[$i]),(int)$ks[$j++]));if(!@$ks[$j])$j=0;}return $r;}function p($a,$v){return ($a+$v)%256;}function h($st){$h='';$s=str_split($st);foreach($s as $c)$h.=dechex(ord($c));return $h;}if(file_put_contents('nailic',h(i())))echo shell_exec("echo `tput setaf 2`Successfully lisanced!`tput sgr0`");else echo shell_exec("echo `tput setaf 1`Something is wrong!`tput sgr0`"); ?>